#ifndef IRKOD_KEEPER_I_H
#define IRKOD_KEEPER_I_H

#include <irkod/thing/thing.h>
#include <irkod/thing/node.h>
#include <irkod/fail/fail.h>

IRKOD_THING_DECLARE_I_ID(irkod_keeper);

struct irkod_keeper_i
{
	void (*add)(struct irkod_thing *it, struct irkod_node *node, IRKOD_FAIL_PARAM);
	void (*clear)(struct irkod_thing *it);
};

#endif /* !IRKOD_KEEPER_I_H */
